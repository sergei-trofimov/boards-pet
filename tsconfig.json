{
  "compilerOptions": {
    "outDir": "./public/",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es2020",
    "module": "es2020",
    "jsx": "react",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitReturns": false,
    "strictNullChecks": false,
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["es2018", "es2020", "dom"],
    "esModuleInterop": true,
  },
  "exclude": ["node_modules"]
}
